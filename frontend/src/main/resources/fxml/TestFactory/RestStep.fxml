<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id="rootRestStep" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wstester.testFactory.RestStepController">
   <children>
      <GridPane fx:id="gridRestStep" hgap="10" vgap="10">
                
          <children>
              <Label text="Step Name:" GridPane.columnIndex="0" GridPane.halignment="RIGHT" />
              <Label fx:id="lblName" prefHeight="17.0" prefWidth="178.0" GridPane.columnIndex="1" />
              <Label text="Rest method:" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
              <Label fx:id="restMethod" prefHeight="17.0" prefWidth="182.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
              
              <Label text="Status:" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="8" />
              <Label fx:id="lblStatus" prefHeight="17.0" prefWidth="180.0" GridPane.columnIndex="1" GridPane.rowIndex="8" />
              
              <Label text="Response:" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="9" />
              <Label fx:id="lblResponse" prefHeight="17.0" prefWidth="179.0" GridPane.columnIndex="1" GridPane.rowIndex="9" />
              <TableView fx:id="tblExecutions" prefHeight="400.0" prefWidth="266.0" GridPane.columnSpan="2" GridPane.rowIndex="12">
      		     <columns>
      		          <TableColumn fx:id="columnDate" text="Date">
		          </TableColumn>
      		          <TableColumn fx:id="columnStatus" text="Status">
		          </TableColumn>
      		          <TableColumn fx:id="columnResponse" prefWidth="104.0" text="Response">
		          </TableColumn>
      		     </columns>
               <GridPane.margin>
                  <Insets left="5.0" right="5.0" />
               </GridPane.margin>
      		</TableView>
            <Label text=" Content type:" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />
            <Label text="Rest  request:" GridPane.halignment="RIGHT" GridPane.rowIndex="7" />
            <Label fx:id="restContentType" prefHeight="17.0" prefWidth="182.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label fx:id="restRequest" prefHeight="17.0" prefWidth="184.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
              
                    <Label text="Past executions" GridPane.halignment="RIGHT" GridPane.rowIndex="10" />
            <Label text="Path:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <Label text="Rest query:" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
            <Label text="Rest cookie:" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
            <Label text="Rest header:" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />
            <Label fx:id="restPath" prefHeight="17.0" prefWidth="173.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="restQuery" prefHeight="17.0" prefWidth="183.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="restCookie" prefHeight="17.0" prefWidth="183.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="restHeader" prefHeight="17.0" prefWidth="184.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveRest" text="Save" GridPane.rowIndex="11" />
          </children>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints minHeight="-Infinity" prefHeight="17.0" />
            <RowConstraints minHeight="-Infinity" prefHeight="17.0" />
            <RowConstraints prefHeight="17.0" />
            <RowConstraints minHeight="-Infinity" prefHeight="17.0" />
            <RowConstraints minHeight="-Infinity" prefHeight="17.0" />
            <RowConstraints />
            <RowConstraints minHeight="-Infinity" prefHeight="17.0" />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints />
         </rowConstraints>
      </GridPane>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</Pane>
